<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".menu_home">

    <ImageButton
        android:id="@+id/imageButtonBusca"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        app:srcCompat="@drawable/salaoimag" />

    <ImageButton
        android:id="@+id/imageButtonDados"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        app:srcCompat="@drawable/cadastroimag" />

    <ImageButton
        android:id="@+id/imageButtonReservas"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        app:srcCompat="@drawable/unicortes" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/logounicortes" />
</LinearLayout>